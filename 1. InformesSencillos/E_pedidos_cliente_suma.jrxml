	<parameter name="Nombre" class="java.lang.String"/>
	<parameter name="Apellido" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT product.name, positions.price, positions.quantity, SUM(positions.price * positions.quantity) AS suma
FROM address, product, positions, document
WHERE (address.id = document.addressid AND document.id = positions.documentid AND positions.productid = product.id)
	AND address.firstname = $P{Nombre}
	AND address.lastname = $P{Apellido}
GROUP BY product.name, positions.price, positions.quantity

]]></query>
