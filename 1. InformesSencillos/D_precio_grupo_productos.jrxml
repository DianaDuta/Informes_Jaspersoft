	<query language="SQL"><![CDATA[SELECT MAX(positions.price) AS Price, address.firstname, product.name, AVG(positions.price) AS Media
FROM positions, address, document, product
WHERE (address.id = document.addressid AND positions.documentid = document.id AND product.id = positions.productid)
GROUP BY product.name, address.firstname
ORDER BY product.name]]></query>
